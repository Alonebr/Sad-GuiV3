-- ITEM FARM --

local Tab = Window:MakeTab({
	Name = " üìÇ Item Farm",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

local Section = Tab:AddSection({
	Name = "Item farm, ao aperte em Start ele inicia e em Stop ele para"
})
Tab:AddButton({
	Name = "Star Item Farm",
	Callback = function()
  			_G.ItemsFarm = true -- Coloque falso para parar e true para iniciar
			while _G.ItemsFarm do wait() 
			---------------------------------------------------- Script

			for i,v in pairs(game:GetService("Workspace"):GetChildren()) do
				if v:IsA("Tool") and v:FindFirstChild("Handle") then
					game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.Handle.CFrame
				end
			end

			---------------------------------------------------- Script
		end -- End lol
	end    
})
Tab:AddButton({
	Name = "Stop Item Farm",
	Callback = function()
  			_G.ItemsFarm = false -- Coloque falso para parar e true para iniciar
			while _G.ItemsFarm do wait() 
			---------------------------------------------------- Script

			for i,v in pairs(game:GetService("Workspace"):GetChildren()) do
				if v:IsA("Tool") and v:FindFirstChild("Handle") then
					game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.Handle.CFrame
				end
			end

			---------------------------------------------------- Script
		end -- End lol
	end    
})
Tab:AddButton({
	Name = "Grab Tools",
	Callback = function()
      	char = game:GetService('Players').LocalPlayer.Character
		game:GetService('Players').LocalPlayer.CharacterAdded:connect(function(body)
		char = body end)
		for i,v in pairs(workspace:GetDescendants()) do
		if v:IsA('Tool') then
		char.Humanoid:EquipTool(v) end end
		workspace.DescendantAdded:connect(function(item)
		pcall(function()
		if item:IsA('Tool') then wait() char.Humanoid:EquipTool(item) end end) end)
  	end    
})

-- END --




