-- LOCAL PLAYER --

local Tab = Window:MakeTab({
	Name = " üôé Local Player",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

local Section = Tab:AddSection({
	Name = "Local Player Scripts"
})
Tab:AddButton({
	Name = "Fly | Togle E ",
	Callback = function()
      		loadstring(game:HttpGet('https://raw.githubusercontent.com/Alonebr/Sad-GuiV3/main/Fly'))()
  	end    
})
Tab:AddButton({
	Name = "Noclip Not Recommended ",
	Callback = function()
      		local character = game.Players.LocalPlayer.Character
			while true do
			for i,v in pairs(character:GetDescendants()) do
			pcall(function()
			if v:IsA("BasePart") then
			v.CanCollide = false
			end
			end)
			end
			task.wait()
			end
  	end    
})
Tab:AddButton({
	Name = "Ctrl + Click Tp ",
	Callback = function()
      		local Plr = game:GetService("Players").LocalPlayer
			local Mouse = Plr:GetMouse()

			Mouse.Button1Down:connect(function()
			if not game:GetService("UserInputService"):IsKeyDown(Enum.KeyCode.LeftControl) then return end
			if not Mouse.Target then return end
			Plr.Character:MoveTo(Mouse.Hit.p)
			end)
  	end    
})
Tab:AddButton({
	Name = "Infinite Jump ",
	Callback = function()
			loadstring(game:HttpGet('https://raw.githubusercontent.com/Alonebr/Sad-GuiV3/main/Infinite%20jump'))()
  	end    
})
Tab:AddSlider({
	Name = "WalkSpeed",
	Min = 16,
	Max = 500,
	Default = 16,
	Color = Color3.fromRGB(255,255,255),
	Increment = 1,
	ValueName = "Speed",
	Callback = function(Speed)
		game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = Speed
	end    
})
Tab:AddSlider({
	Name = "JumpPower",
	Min = 50,
	Max = 500,
	Default = 24,
	Color = Color3.fromRGB(255,255,255),
	Increment = 1,
	ValueName = "Jump",
	Callback = function(JumpPW)
		game.Players.LocalPlayer.Character.Humanoid.JumpPower = JumpPW
	end    
})


