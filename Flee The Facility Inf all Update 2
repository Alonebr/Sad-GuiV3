-- Scripts:

local function UTYPK_fake_script() -- Menu.LocalScript 
	local script = Instance.new('LocalScript', Menu)

	script.Parent.close.MouseButton1Click:Connect(function()
		script.Parent.Parent.Menu.Visible = false
	end)
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	local UIS = game:GetService('UserInputService')
	
	local frame = script.Parent.Parent.Menu
	
	
	
	local dragToggle = nil
	
	local dragSpeed = 0.25
	
	local dragStart = nil
	
	local startPos = nil
	
	
	
	local function updateInput(input)
	
		local delta = input.Position - dragStart
	
		local position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X,
	
			startPos.Y.Scale, startPos.Y.Offset + delta.Y)
	
		game:GetService('TweenService'):Create(frame, TweenInfo.new(dragSpeed), {Position = position}):Play()
	
	end
	
	
	
	frame.InputBegan:Connect(function(input)
	
		if (input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch) then 
	
			dragToggle = true
	
			dragStart = input.Position
	
			startPos = frame.Position
	
			input.Changed:Connect(function()
	
				if input.UserInputState == Enum.UserInputState.End then
	
					dragToggle = false
	
				end
	
			end)
	
		end
	
	end)
	
	
	
	UIS.InputChanged:Connect(function(input)
	
		if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
	
			if dragToggle then
	
				updateInput(input)
	
			end
	
		end
	
	end)
end
coroutine.wrap(UTYPK_fake_script)()
local function CJANDSC_fake_script() -- radio_button_unchecked.LocalScript 
	local script = Instance.new('LocalScript', radio_button_unchecked)

	script.Parent.Parent.radio_button_unchecked.MouseButton1Click:Connect(function()
		script.Parent.Parent.radio_button_checked.Visible = true
		wait(0.1)
		game.Players.LocalPlayer.TempPlayerStatsModule.Health.Value = "1"
	end)
end
coroutine.wrap(CJANDSC_fake_script)()
local function JXIJDD_fake_script() -- radio_button_checked.LocalScript 
	local script = Instance.new('LocalScript', radio_button_checked)

	script.Parent.Parent.radio_button_checked.MouseButton1Click:Connect(function()
		script.Parent.Parent.radio_button_checked.Visible = false
	end)
end
coroutine.wrap(JXIJDD_fake_script)()
