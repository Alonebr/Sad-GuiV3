print("Project Stars loaded")

local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = OrionLib:MakeWindow({Name = "Project Stars World 1", HidePremium = false, SaveConfig = true, ConfigFolder = "OrionTest", IntroEnable = true, IntroText = "  |  Project Stars World 1 ‚≠ê ", IntroIcon = "http://www.roblox.com/asset/?id=11822586739"})

--> Welcome
local Tab = Window:MakeTab({
	Name = " üëã Welcome",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})
Tab:AddLabel("Welcome to Project Stars")

--> Summon

local Tab = Window:MakeTab({
	Name = " üìÇ Summons",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

local Section = Tab:AddSection({
	Name = "Banner X"
})
Tab:AddButton({
	Name = "Summon 1 (Need 50 gems)",
	Callback = function()
      	local args = {
            [1] = "SummonStart",
            [2] = 1
        }

        game:GetService("ReplicatedStorage").Remotes.Server:InvokeServer(unpack(args))
  	end    
})
Tab:AddButton({
	Name = "Summon 10 (Need 450 gems)",
	Callback = function()
        local args = {
            [1] = "SummonStart10",
            [2] = 1
        }

        game:GetService("ReplicatedStorage").Remotes.Server:InvokeServer(unpack(args))
  	end    
})



