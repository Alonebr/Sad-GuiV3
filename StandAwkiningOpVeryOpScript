local Tab = Window:CreateTab("Item Farm")
local Paragraph = Tab:CreateParagraph({Title = "Bem vindo ao item Farm", Content = "Ao aperta em Star ele iniciar√° e no stop ele ir√° parar"})

local Button = Tab:CreateButton({
	Name = "Start Item Farm",
	Callback = function()
		  			_G.ItemsFarm = true -- Coloque falso para parar e true para iniciar
			while _G.ItemsFarm do wait() 
			---------------------------------------------------- Script

			for i,v in pairs(game:GetService("Workspace"):GetChildren()) do
				if v:IsA("Tool") and v:FindFirstChild("Handle") then
					game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.Handle.CFrame
				end
			end

			---------------------------------------------------- Script
		end -- End lol
	end,
})
local Button = Tab:CreateButton({
	Name = "Stop Item Farm",
	Callback = function()
		  	_G.ItemsFarm = false -- Coloque falso para parar e true para iniciar
			while _G.ItemsFarm do wait() 
			---------------------------------------------------- Script

			for i,v in pairs(game:GetService("Workspace"):GetChildren()) do
				if v:IsA("Tool") and v:FindFirstChild("Handle") then
					game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.Handle.CFrame
				end
			end

			---------------------------------------------------- Script
		end -- End lol
	end,
})

-- END --




