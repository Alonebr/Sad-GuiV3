	Name = "Main Scripts"
})
Tab:AddButton({
	Name = "Auto Block",
	Callback = function()
      	local args = {
			[1] = "Alternate",
			[2] = "Block"
		}

		game:GetService("ReplicatedStorage").Main.Input:FireServer(unpack(args))
  		end    
})
Tab:AddButton({
	Name = "Infinite Damage Reflect",
	Callback = function()

		OrionLib:MakeNotification({
		Name = "Cuidado",
		Content = "Requer Gold Expirience Requiem",
		Image = "rbxassetid://4483345998",
		Time = 5
	})

wait(2)

		local args = {
			[1] = "Alternate",
			[2] = "RTZ",
			[3] = true
		}

		game:GetService("ReplicatedStorage").Main.Input:FireServer(unpack(args))
		end  

})
Tab:AddButton({
	Name = "Infinite Pose",
	Callback = function()
		local args = {
			[1] = true
		}

		game:GetService("ReplicatedStorage").Main.Menacing:FireServer(unpack(args))
  		end    
})

-- END --




