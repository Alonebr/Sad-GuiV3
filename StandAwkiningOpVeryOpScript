local function DYPQW_fake_script() -- Insdamagereflection.LocalScript 
	local script = Instance.new('LocalScript', Insdamagereflection)

	script.Parent.Parent.Insdamagereflection.MouseButton1Click:Connect(function()
		if game:GetService("Workspace").FilteringEnabled == true then
			game:GetService("StarterGui"):SetCore("SendNotification", {
				Title = " cuidado ";
				Text = " Requer Gold Expirience Requiem ";
			})
		end
		
		wait(2)
	
		local args = {
			[1] = "Alternate",
			[2] = "RTZ",
			[3] = true
		}
	
		game:GetService("ReplicatedStorage").Main.Input:FireServer(unpack(args))
	
	end)
end
coroutine.wrap(DYPQW_fake_script)()
