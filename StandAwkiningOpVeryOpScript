	PremiumOnly = false
})

local Section = Tab:AddSection({
	Name = "Clique em um para se teletransporta"
})
Tab:AddButton({
	Name = "Bank",
	Callback = function()
  		game.Players.LocalPlayer.Character:MoveTo(Vector3.new(1342.28, 584.95, -546.44))
	end    
})
