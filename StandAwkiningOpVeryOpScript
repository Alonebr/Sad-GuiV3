local function BCUPXGO_fake_script() -- ShadowDioTimeStop.LocalScript 
	local script = Instance.new('LocalScript', ShadowDioTimeStop)

	script.Parent.Parent.ShadowDioTimeStop.MouseButton1Click:Connect(function()
		
		local args = {
			[1] = 20000000,           -- 20000000 = 20 or 15
			[2] = "shadowdio"			-- Shadow dio / Stw
		}
	
		game:GetService("ReplicatedStorage").Main.Timestop:FireServer(unpack(args))
		
	end)
end
coroutine.wrap(BCUPXGO_fake_script)()
local function GKTX_fake_script() -- JSPTimeStop.LocalScript 
	local script = Instance.new('LocalScript', JSPTimeStop)

	script.Parent.Parent.JSPTimeStop.MouseButton1Click:Connect(function()
		script.Parent.Parent.ShadowDioTimeStop.Visible = false
		
		-- Script generated by SimpleSpy - credits to exx#9394
	
		local args = {
			[1] = 200000000000,
			[2] = "jotaro"
		}
	
		game:GetService("ReplicatedStorage").Main.Timestop:FireServer(unpack(args))
	
	end)
end
coroutine.wrap(GKTX_fake_script)()
local function RFQWL_fake_script() -- MainScriptsHub.LocalScript 
	local script = Instance.new('LocalScript', MainScriptsHub)

	script.Parent.Parent.MainScriptsHub.MouseButton1Click:Connect(function()
		script.Parent.Parent.Stands.Visible = false
		script.Parent.Parent.MainScripts.Visible = true
		script.Parent.Parent.JSPTimeStop.Visible = false
		script.Parent.Parent.ShadowDioTimeStop.Visible = false
		script.Parent.Parent.EVAONETimseStop.Visible = false
	end)
end
