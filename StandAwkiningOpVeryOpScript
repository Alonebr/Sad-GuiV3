	Image = "rbxassetid://4483345998",
	Time = 5
})

-- END --





-- STANDS --

local Tab = Window:MakeTab({
	Name = "Stands",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})
local Section = Tab:AddSection({
	Name = "Selecione seu Stand, que o seu time stop tera 20 segundos"
})
Tab:AddButton({
	Name = "Shadow Dio",
	Callback = function()
      		local args = {
				[1] = 20000000,           -- 20000000 = 20 or 15
				[2] = "shadowdio"			-- Shadow dio / Stw
			}

			game:GetService("ReplicatedStorage").Main.Timestop:FireServer(unpack(args))
  			end    
})
Tab:AddButton({
	Name = "JSP",
	Callback = function()
      		local args = {
				[1] = 20000000,           -- 20000000 = 20 or 15
				[2] = "jotaro"			-- Shadow dio / Stw
			}

			game:GetService("ReplicatedStorage").Main.Timestop:FireServer(unpack(args))
  			end    
})
Tab:AddButton({
	Name = "EVA 01",
	Callback = function()
      		local args = {
				[1] = 20000000,           -- 20000000 = 20 or 15
				[2] = "diooh"			-- Shadow dio / Stw
			}

			game:GetService("ReplicatedStorage").Main.Timestop:FireServer(unpack(args))
  			end    
})
local Section = Tab:AddSection({
	Name = "D4C Clones, ao aperta ele vai spawna 1 clone"
})
Tab:AddButton({
	Name = "D4C Clones",
	Callback = function()
			local args = {
				[1] = "Alternate",
				[2] = "Clone"
			}	

		game:GetService("ReplicatedStorage").Main.Input:FireServer(unpack(args)) 
		end  
})

-- END --





