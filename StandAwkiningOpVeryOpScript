-- Scripts:

local function VJSATIZ_fake_script() -- MainFrame.Main 
	local script = Instance.new('LocalScript', MainFrame)

	--[[
			Stand Awekining Op script
			made By Darkzin
	--]]
	
	
	
	
	
	-- Funções
	
	local UIS = game:GetService('UserInputService')
	
	local frame = script.Parent.Parent.MainFrame
	
	
	
	local dragToggle = nil
	
	local dragSpeed = 0.25
	
	local dragStart = nil
	
	local startPos = nil
	
	
	
	local function updateInput(input)
	
		local delta = input.Position - dragStart
	
		local position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X,
	
			startPos.Y.Scale, startPos.Y.Offset + delta.Y)
	
		game:GetService('TweenService'):Create(frame, TweenInfo.new(dragSpeed), {Position = position}):Play()
	
	end
	
	
	
	frame.InputBegan:Connect(function(input)
	
		if (input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch) then 
	
			dragToggle = true
	
			dragStart = input.Position
	
			startPos = frame.Position
	
			input.Changed:Connect(function()
	
				if input.UserInputState == Enum.UserInputState.End then
	
					dragToggle = false
	
				end
	
			end)
	
		end
	
	end)
	
	
	
	UIS.InputChanged:Connect(function(input)
	
		if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
	
			if dragToggle then
	
				updateInput(input)
	
			end
	
		end
	
	end)
	
	
	
	
	
	
	-- Porpriedades
	
	local Plr = game.Players.LocalPlayer
	
	Plr:GetMouse().KeyDown:Connect(function(E)
		if E == "e" then
			script.Parent.Parent.MainFrame.Visible = true
		end
	end)
	
	Plr:GetMouse().KeyDown:Connect(function(Q)
		if Q == "q" then
			script.Parent.Parent.MainFrame.Visible = false
		end
	end)
	
end
coroutine.wrap(VJSATIZ_fake_script)()
local function JUZNGE_fake_script() -- ShadowDioSelected.LocalScript 
	local script = Instance.new('LocalScript', ShadowDioSelected)

	script.Parent.Parent.ShadowDioSelected.MouseButton1Click:Connect(function()
		script.Parent.Parent.Parent.ShadowDioTimeStop.Visible = true
		script.Parent.Parent.Parent.JSPTimeStop.Visible = false
	end)
end
coroutine.wrap(JUZNGE_fake_script)()
local function DCWQAIC_fake_script() -- JSPSelected.LocalScript 
	local script = Instance.new('LocalScript', JSPSelected)

	script.Parent.Parent.JSPSelected.MouseButton1Click:Connect(function()
		script.Parent.Parent.Parent.JSPTimeStop.Visible = true
		script.Parent.Parent.Parent.ShadowDioTimeStop.Visible = false
	end)
end
coroutine.wrap(DCWQAIC_fake_script)()
local function FLKJYZV_fake_script() -- info.LocalScript 
	local script = Instance.new('LocalScript', info)

	script.Parent.Parent.info.MouseButton1Click:Connect(function()
		if game:GetService("Workspace").FilteringEnabled == true then
			game:GetService("StarterGui"):SetCore("SendNotification", {
				Title = " INFO ";
				Text = " aqui voce seleciona o seu stand (que contem time stop) e o time stop dele vai ter 20 segundos ";
			})
		end
	end)
end
coroutine.wrap(FLKJYZV_fake_script)()
local function GSFB_fake_script() -- ShadowDioTimeStop.LocalScript 
	local script = Instance.new('LocalScript', ShadowDioTimeStop)

	script.Parent.Parent.ShadowDioTimeStop.MouseButton1Click:Connect(function()
		
		local args = {
			[1] = 20000000,           -- 20000000 = 20 or 15
			[2] = "shadowdio"			-- Shadow dio / Stw
		}
	
		game:GetService("ReplicatedStorage").Main.Timestop:FireServer(unpack(args))
		
	end)
end
coroutine.wrap(GSFB_fake_script)()
local function RAYUCMN_fake_script() -- JSPTimeStop.LocalScript 
	local script = Instance.new('LocalScript', JSPTimeStop)

	script.Parent.Parent.JSPTimeStop.MouseButton1Click:Connect(function()
		script.Parent.Parent.ShadowDioTimeStop.Visible = false
		
		-- Script generated by SimpleSpy - credits to exx#9394
	
		local args = {
			[1] = 200000000000,
			[2] = "jotaro"
		}
	
		game:GetService("ReplicatedStorage").Main.Timestop:FireServer(unpack(args))
	
	end)
end
coroutine.wrap(RAYUCMN_fake_script)()
