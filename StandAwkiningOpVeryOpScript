local function RZUXEV_fake_script() -- ShadowDioTimeStop.LocalScript 
	local script = Instance.new('LocalScript', ShadowDioTimeStop)

	script.Parent.Parent.ShadowDioTimeStop.MouseButton1Click:Connect(function()
		
		local args = {
			[1] = 20000000,           -- 20000000 = 20 or 15
			[2] = "shadowdio"			-- Shadow dio / Stw
		}
	
		game:GetService("ReplicatedStorage").Main.Timestop:FireServer(unpack(args))
		
	end)
end
