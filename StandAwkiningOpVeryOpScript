local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()

local Window = OrionLib:MakeWindow({Name = "Project Awakening v3.0 ðŸ¥³", HidePremium = false, SaveConfig = true, ConfigFolder = "OrionTest"})

-- MENSAGEM --

OrionLib:MakeNotification({
	Name = "Creditos !",
