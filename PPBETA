--// Script \\--
local Rayfield = loadstring(game:HttpGet('https://raw.githubusercontent.com/shlexware/Rayfield/main/source'))()
local Window = Rayfield:CreateWindow({
   Name = "Project Piece  |  @ProjectHub",
   LoadingTitle = "Project Hub",
   LoadingSubtitle = "@Darkzin  |  @Coold",
   ConfigurationSaving = {
      Enabled = true,
      FolderName = nil, -- Create a custom folder for your hub/game
      FileName = "Big Hub"
   },
   Discord = {
      Enabled = false,
      Invite = "sirius", -- The Discord invite code, do not include discord.gg/
      RememberJoins = true -- Set this to false to make them join the discord every time they load it up
   },
   KeySystem = true, -- Set this to true to use our key system
   KeySettings = {
      Title = "Project Hub",
      Subtitle = "Key System",
      Note = "Join the discord (Beta)",
      FileName = "SiriusKey",
      SaveKey = true,
      GrabKeyFromSite = false, -- If this is true, set Key below to the RAW site you would like Rayfield to get the key from
      Key = "9ebbd1803758011356fb53c3939ccc2daf00898e"
   }
})



--// Tabs \\--


--// Welcome \\--
local Tab = Window:CreateTab(" ðŸ‘‹ Welcome")
local Paragraph = Tab:CreateParagraph({Title = "Bem Vindo", Content = "Seja bem vindo ao Project Piece"})



--// Roll \\--
local Tab = Window:CreateTab("Fruits")
local Paragraph = Tab:CreateParagraph({Title = "Roll Fruit", Content = "Aqui vocÃª vai poder roda a fruta sem cooldown"})
local Button = Tab:CreateButton({
   Name = "Fruit Roll No Cooldown",
   Callback = function()
        local args = {
            [1] = "SPINB"
        }
        
        game:GetService("ReplicatedStorage").Remotes.SpinsRE:FireServer(unpack(args))
        
        wait(0.1)
        
        local args = {
            [1] = "GIVE"
        }
        
        game:GetService("ReplicatedStorage").Remotes.SpinsRE:FireServer(unpack(args))
   end,
})



--// Main \\--
local Tab = Window:CreateTab("Main")
local Paragraph = Tab:CreateParagraph({Title = "Main", Content = "Main Scripts"})


local Section = Tab:CreateSection("")


local Button = Tab:CreateButton({
   Name = "Close Attacks",
   Callback = function()
       game:GetService("CoreGui").Rayfield.Main.Elements.Main.Z:Destroy()
       game:GetService("CoreGui").Rayfield.Main.Elements.Main.X:Destroy()
       game:GetService("CoreGui").Rayfield.Main.Elements.Main.C:Destroy()
       game:GetService("CoreGui").Rayfield.Main.Elements.Main.V:Destroy()
   end,
})
local Dropdown = Tab:CreateDropdown({
   Name = "Fruits Attack",
   Options = {"StarPlatinumAttacks","BarrierAttacks"},
   CurrentOption = "",
   Flag = "Dropdown1", -- A flag is the identifier for the configuration file, make sure every element has a different flag if you're using configuration saving to ensure no overlaps
   Callback = function(Option)
        if Options == StarPlatinumAttacks then 

        local Button = Tab:CreateButton({
           Name = "Z",
           Callback = function()
                local A_1 = "StarPlatinum"
                local A_2 = "Z"
                local Event = game:GetService("ReplicatedStorage").Remotes.Cast
                Event:FireServer(A_1, A_2)
           end,
        })
        local Button = Tab:CreateButton({
           Name = "X",
           Callback = function()
                local A_1 = "StarPlatinum"
                local A_2 = "X"
                local Event = game:GetService("ReplicatedStorage").Remotes.Cast
                Event:FireServer(A_1, A_2)
           end,
        })
        local Button = Tab:CreateButton({
           Name = "C",
           Callback = function()
                local A_1 = "StarPlatinum"
                local A_2 = "C"
                local Event = game:GetService("ReplicatedStorage").Remotes.Cast
                Event:FireServer(A_1, A_2)
           end,
        })
        local Button = Tab:CreateButton({
           Name = "V",
           Callback = function()
                local A_1 = "StarPlatinum"
                local A_2 = "V"
                local Event = game:GetService("ReplicatedStorage").Remotes.Cast
                Event:FireServer(A_1, A_2)
           end,
        })

        elseif Options == BarrierAttacks then
        local Button = Tab:CreateButton({
            Name = "BariZ",
            Callback = function()
                local A_1 = "Bari"
                local A_2 = "Z"
                local Event = game:GetService("ReplicatedStorage").Remotes.Cast
                Event:FireServer(A_1, A_2)
            end,
        })
        local Button = Tab:CreateButton({
            Name = "BariX",
            Callback = function()
                local A_1 = "Bari"
                local A_2 = "X"
                local Event = game:GetService("ReplicatedStorage").Remotes.Cast
                Event:FireServer(A_1, A_2)
            end,
        })
        local Button = Tab:CreateButton({
            Name = "BariC",
            Callback = function()
                local A_1 = "Bari"
                local A_2 = "C"
                local Event = game:GetService("ReplicatedStorage").Remotes.Cast
                Event:FireServer(A_1, A_2)
            end,
        })
        end

    end,
})
